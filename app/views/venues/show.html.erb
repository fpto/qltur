  <div class = "row">
    <div class="container">
      <div class = "col-md-6">
        <h1><%= @venue.name %></h1>
        <p> <%= @venue.address1 %> </p>
        <p> <%= @venue.description %> </p>
        <p> <%= @cleanAddress %> </p>
      </div>

    <div class = "col-md-6">
      <iframe
        width="600"
        height="450"
        frameborder="0" style="border:0"
        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCiY34QREeRPpC4AqOFUw8nd_nyGh1i1xY  &q=<%= @cleanAddress1 %>,<%=@cleanPostalcode%>,<%=@cleanCity%>,<%=@cleanCountry%>" allowfullscreen>
      </iframe>
    </div>
</div>
<div class = "container">
  <h2> Coming up </h2>
  <div class = "row">
    <div class = "container">
      <% @concerts.each do |concert| %> 
        <div class="col-md-3">
          <div class="venue" style="background-image: url(<%= concert.image_url %>); background-size: cover;  "> 
            <p class="title"><%= concert.title %></p> 
            <p class="description"><%= concert.description %></p>
          </div> 
        </div>
      <% end %>
    </div>
  </div>
</div>
